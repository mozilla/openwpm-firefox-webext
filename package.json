  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "eslint-plugin-import": "^2.5.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mozilla": "^0.14.0",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "npm-run-all": "^4.1.1",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.4",
    "typescript-eslint-parser": "^20.0.0",
    "web-ext": "^2.7.0",
    "webpack": "^4.1.0",
    "webpack-cli": "^3.1.2"
  },
  "engines": {
    "node": ">=8.11.1"
  },
  "homepage": "",
  "keywords": [
    "firefox",
    "mozilla",
    "openwpm"
  ],
  "license": "MPL-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/nhnt11/WEE-Sockets-API-Example"
  },
  "scripts": {
    "prebuild": "cp -r node_modules/WEE-Sockets-API/lib/* src/ && cp -r node_modules/WEE-FileWriting-API/lib/privileged/* src/privileged/ && webpack",
